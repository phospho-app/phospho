            "source": [
                "## Preprocessing\n",
                "\n",
                "Collect sessions and embed them in a vector store"
            ]
        },
        {
            "cell_type": "code",
            "execution_count": 6,
            "metadata": {},
            "outputs": [
                {
                    "name": "stderr",
                    "output_type": "stream",
                    "text": [
                        "Semantic search: Searching only through in steps whose name is in:Start a conversation, Continue conversation, Conversation starter, response-generation, conversation starter, continue conversation, main step, gpt3-call\n"
                    ]
                }
            ],
            "source": [
                "from loguru import logger\n",
                "from typing import Any, Dict, Generator, List, Optional\n",
                "\n",
                "import pandas as pd\n",
                "from google.cloud.firestore import DocumentSnapshot\n",
                "from llama_index import Document, VectorStoreIndex\n",
                "from llama_index.node_parser import SimpleNodeParser\n",
                "\n",
                "from app.db.client import client as db\n",
                "\n",
                "# Not all steps are LLM calls. We will perform semantic search only on steps\n",
                "# with the following names:\n",
                "LLM_CALLS_STEP_NAMES = [\n",
                "    \"Start a conversation\",\n",
                "    \"Continue conversation\",\n",
                "    \"Conversation starter\",\n",
                "    \"response-generation\",\n",
                "    \"conversation starter\",\n",
                "    \"continue conversation\",\n",
                "    \"main step\",\n",
                "    \"gpt3-call\",\n",
                "]\n",
                "# Placeholder for a vector database\n",
                "# TODO : use a Real one\n",
                "vector_database: Dict[str, List[float]] = {}\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "\n",
                "\"\"\"\n",
                "Perform semantic search for certain topics in chat sessions.\n",
                "\n",
                "search_query (str): The topic to look for.\n",
                "    Example: 'Give me sessions about toys', 'dino stuff', 'big fails'\n",
                "project_id (Optional[str]): The id of the project to limit the search to.\n",
                "max_nb_of_docs (Optional[int]): Maximum Number of documents to retrieve.\n",
                "\n",
                "Returns List[session_id: str] a list of relevant session id\n",
                "\"\"\"\n",
                "\n",
                "# Fetching data from db and merging everything together\n",
                "# !\ This step could be optimized by:\n",
                "# - using an SQL database (or a copy of it)\n",
                "# - dumping more data in the steps and tasks to filter more quickly\n",
                "\n",
                "# Retrieve all steps\n",
                "all_steps_docs: Generator[DocumentSnapshot, Any, None] = db.get_collection(\n",
                "    \"steps\"\n",
                ").stream()\n",
                "steps_df = pd.DataFrame(\n",
                "    [{\"step_id\": doc.id, **doc.to_dict()} for doc in all_steps_docs]\n",
                ")\n",
                "\n",
                "# Retrieve all tasks\n",
                "all_tasks_docs: Generator[DocumentSnapshot, Any, None] = db.get_collection(\n",
                "    \"tasks\"\n",
                ").stream()\n",
                "tasks_df = pd.DataFrame(\n",
                "    [{\"task_id\": doc.id, **doc.to_dict()} for doc in all_tasks_docs]\n",
                ")\n",
                "\n",
                "# Merge tasks, steps\n",
                "merged_df = steps_df.merge(\n",
                "    tasks_df,\n",
                "    on=\"task_id\",\n",
                "    suffixes=(\"_step\", \"_task\"),\n",
                "    validate=\"m:1\",  # There is 1 task for every step\n",
                ")\n",
                "\n",
                "if project_id:\n",
                "    # If a project_id is specified, filter the dataframe to only keep data related\n",
                "    # to the project_id\n",
                "    all_sessions_docs: Generator[DocumentSnapshot, Any, None] = db.simple_query(\n",
                "        \"sessions\", \"project_id\", \"==\", project_id\n",
                "    )\n",
                "else:  # Otherwise take all projects\n",
                "    all_sessions_docs = db.get_collection(\"sessions\").stream()\n",
                "sessions_df = pd.DataFrame(\n",
                "    [{\"session_id\": doc.id, **doc.to_dict()} for doc in all_sessions_docs]\n",
                ")\n",
                "\n",
                "# Merge sessions, steps\n",
                "merged_df = merged_df.merge(\n",
                "    sessions_df,\n",
                "    on=\"session_id\",\n",
                "    validate=\"m:1\",  # There is 1 sessions for every task\n",
                ")\n",
                "\n",
                "# Not all steps are LLM calls\n",
                "# Filter those based on the step name\n",
                "# TODO : Revise the database schema and logging of data to make this step non-ambiguous\n",
                "logger.warning(\n",
                "    \"Semantic search: Searching only through in steps whose name is in:\"\n",
                "    + \", \".join(LLM_CALLS_STEP_NAMES)\n",
                ")\n",
                "chat_sessions_df = (\n",
                "    merged_df[\n",
                "        merged_df[\"name\"].str.lower().isin(LLM_CALLS_STEP_NAMES)\n",
                "        # Additional filtering to avoid test queries\n",
                "        & (merged_df[\"input_step\"] != \"string\")\n",
                "        & (merged_df[\"output_step\"] != \"Successfully streamed output\")\n",
                "    ]\n",
                "    .sort_values(by=\"created_at_step\", ascending=True)\n",
                "    .groupby(\"session_id\")\n",
                "    .agg(\n",
                "        {\n",
                "            \"task_id\": list,\n",
                "            \"step_id\": list,\n",
                "            \"input_step\": list,\n",
                "            \"output_step\": list,\n",
                "            \"name\": list,\n",
                "        }\n",
                "    )\n",
                "    .reset_index()\n",
                ")\n",
                "\n",
                "# Convert sessions to documents for compatibility with Llama index\n",
                "# 1 Session = 1 Document\n",
                "# This steps converts the session into a string representation to create embeddings of it.\n",
                "documents = []\n",
                "for i in range(chat_sessions_df.shape[0]):\n",
                "    row = chat_sessions_df.iloc[i]\n",
                "    # Format the text as a series of user and assistant replies\n",
                "    text = \"\\n\".join(\n",
                "        [\n",
                "            f\"User: {row['input_step'][i]}\\nAssistant: {row['output_step'][i]}\"\n",
                "            for i, input_msg in enumerate(row[\"input_step\"])\n",
                "        ]\n",
                "    )\n",
                "\n",
                "    doc = Document(text=text)\n",
                "    doc.metadata = row[[\"session_id\", \"task_id\", \"step_id\", \"name\"]].to_dict()\n",
                "    documents.append(doc)\n",
                "\n",
                "parser = SimpleNodeParser()\n",
                "nodes = parser.get_nodes_from_documents(documents)\n",
                "\n",
                "for node in nodes:\n",
                "    text = node.get_content()\n",
                "    # Look if we already have an embedding for this text\n",
                "    embedding = vector_database.get(text, None)\n",
                "    node.embedding = embedding\n",
                "\n",
                "# Generate the index (take some time)\n",
                "logger.info(f\"Generating an index from {len(documents)} sessions\")\n",
                "index = VectorStoreIndex(nodes=nodes)\n",
                "\n",
                "# Save the generated embeddings to the vector store\n",
                "nodes_with_embedding = index._get_node_with_embedding(nodes)\n",
                "for node in nodes_with_embedding:\n",
                "    text = node.get_content()\n",
                "    if text not in vector_database.keys():\n",
                "        if node.embedding is not None:\n",
                "            vector_database[text] = node.embedding\n",
                "\n",
                "\n",
                "# Filter for min similarity\n",
                "def good_enough_score(doc):\n",
                "    \"\"\"Filtering function to keep only the docs with score above threshold\"\"\"\n",
                "    if doc.score is not None:\n",
                "        if doc.score >= min_similarity_score:\n",
                "            if project_id is not None:\n",
                "                # Make sure that we only keep doc relevant to project_id\n",
                "                if doc.metadata[\"project_id\"] == project_id:\n",
                "                    return True\n",
                "            else:\n",
                "                return True\n",
                "    return False\n",
                "\n",
                "max_nb_of_docs = 5\n",
                "min_similarity_score = 0.5\n",
                "retriever = index.as_retriever(similarity_top_k=max_nb_of_docs)\n"
            ]
        },
        {
